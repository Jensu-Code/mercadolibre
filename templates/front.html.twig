<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Title -->
    <title>{% block title %}Welcome!{% endblock %}</title>
    <!-- Favicon -->
    <link rel="shortcut icon" href="{{ asset('build/images/logo/favicon-two.png') }}">

    {% block stylesheets %}{% endblock %}
    {{ encore_entry_link_tags('app') }}

    {{ encore_entry_script_tags('app') }}
    {% block javascripts %} {% endblock %}
</head>
<body>

<!--==================== Preloader Start ====================-->
<div class="loader-mask">
    <div class="loader">
        <div></div>
        <div></div>
    </div>
</div>
<!--==================== Preloader End ====================-->

<!--==================== Overlay Start ====================-->
<div class="overlay"></div>
<!--==================== Overlay End ====================-->

<!--==================== Sidebar Overlay End ====================-->
<div class="side-overlay"></div>
<!--==================== Sidebar Overlay End ====================-->

<!-- ==================== Scroll to Top End Here ==================== -->
<div class="progress-wrap">
    <svg class="progress-circle svg-content" width="100%" height="100%" viewBox="-1 -1 102 102">
        <path d="M50,1 a49,49 0 0,1 0,98 a49,49 0 0,1 0,-98" style="transition: stroke-dashoffset 10ms linear 0s; stroke-dasharray: 307.919, 307.919; stroke-dashoffset: 138.713;"></path>
    </svg>
</div>
<!-- ==================== Scroll to Top End Here ==================== -->

<!-- ==================== Mobile Menu Start Here ==================== -->
{% include 'header_mobile.html.twig' %}
<!-- ==================== Mobile Menu End Here ==================== -->

<main class="change-gradient">
    {% include 'header.html.twig' %}

    {% block body %}{% endblock %}

    {% include 'footer.html.twig' %}
</main>

</body>
</html>

